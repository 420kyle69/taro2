    <!-- Game Editor -->
    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script>
        let initEditor = false;
        window.inGameEditor = {
            // this object will be filled from in-game editor as per need
        };
        function loadEditor() {
            if (initEditor) {
                return;
            }
            if (false) {
                const scriptEl = document.createElement('script');
                scriptEl.src = `http://localhost:3001/bundle.js`;
                scriptEl.id = 'editor-script';
                document.body.appendChild(scriptEl);
                initEditor = true;
                return;
            }

            const rootUrl = 'https://www.modd.io';
            // const rootUrl = 'http://localhost:3000';
            // const engineVersion = '2.0.0';

            axios.get(`${rootUrl}/client/latest/`)
                .then((res) => {
                    const { styles, scripts, version } = res.data;

                    styles.forEach((style) => {
                        const linkEl = window.document.createElement('link');
                        linkEl.rel = 'stylesheet';
                        linkEl.href = `${rootUrl}/${version}/static/css/` + style;
                        window.document.head.appendChild(linkEl);
                    });
                    scripts.forEach((script) => {
                        const scriptEl = window.document.createElement('script');
                        scriptEl.src = `${rootUrl}/${version}/static/js/` + script;
                        window.document.body.appendChild(scriptEl);
                    });
                    window.initEditor = true;
                })
                .catch((err) => {
                    console.log(err);
                });

            initEditor = true;
        }
    </script>
